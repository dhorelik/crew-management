[ignore]
.*/node_modules/.*
.*/build/.*
.*/registerServiceWorker.js

[include]

[libs]
flow-typed

[lints]

[options]
all=true

module.name_mapper='^components/\(.*\)$' -> '<PROJECT_ROOT>/src/components/\1'
module.name_mapper='^constants/\(.*\)$' -> '<PROJECT_ROOT>/src/constants/\1'
module.name_mapper='^middleware/\(.*\)$' -> '<PROJECT_ROOT>/src/middleware/\1'
module.name_mapper='^screens$' -> '<PROJECT_ROOT>/src/screens'
module.name_mapper='^store/\(.*\)$' -> '<PROJECT_ROOT>/src/store/\1'
module.name_mapper='^styles/\(.*\)$' -> '<PROJECT_ROOT>/src/styles/\1'
module.name_mapper='^types/\(.*\)$' -> '<PROJECT_ROOT>/src/types/\1'

module.name_mapper='^reducer$' -> '<PROJECT_ROOT>/src/reducer'
module.name_mapper='^initialState$' -> '<PROJECT_ROOT>/src/initialState'

;TODO remove that, data hack is temporary
module.name_mapper='^data.json$' -> '<PROJECT_ROOT>/src/data.json'

[strict]
